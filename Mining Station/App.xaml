<Application
    x:Class="Mining_Station.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Mining_Station"
    DispatcherUnhandledException="Application_DispatcherUnhandledException"
    Startup="Application_Startup"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles.xaml" />
                <ResourceDictionary Source="Themes/NumericUpDown.xaml" />
                <ResourceDictionary Source="Themes/NumericUpDownTime.xaml" />
                <ResourceDictionary Source="Themes/PasswordBoxPlus.xaml" />
                <ResourceDictionary Source="Themes/RangeSlider.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
